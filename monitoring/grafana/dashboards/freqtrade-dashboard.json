{
  "dashboard": {
    "id": null,
    "title": "Freqtrade Multi-Bot System",
    "tags": ["freqtrade", "trading", "bots"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Management Server - HTTP Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"management_server\"}[5m])",
            "legendFormat": "{{method}} {{status_code}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Trading Gateway - HTTP Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"trading_gateway\"}[5m])",
            "legendFormat": "{{method}} {{status_code}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Redis Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "API Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s"
  }
}